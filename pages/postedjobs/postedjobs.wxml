

<view class="saved_card">
<view class="touch-item {{item.isTouchMove ? 'touch-move-active' : ''}}" data-index="{{index}}" bindtouchstart="touchstart" bindtouchmove="touchmove" wx:for="{{items}}" wx:key="" bindlongtap='click'data-id= "{{item.job_id}}">

    <image class="job_logo" src="{{item.image}}"/>
    <view class="job_info">
      <view class="job_title">{{item.job_title}}</view>
      <view class="company_name">{{item.company_name}}</view>
    </view>

  <view class="del" catchtap="del" data-index="{{index}}">delete</view>
 </view>
</view>

