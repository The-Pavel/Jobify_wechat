.square {
  /* background: #0245AC; */
  width: 500rpx;
  height: 500rpx;
  position: relative;
  margin: 50vh auto;
  transform: translateY(-50%);
  overflow: hidden;
  animation: movinInBlock 1s ease-out;
}
.square view {
  position: absolute;
   background: #8FBAF3; 
}
.square .line-top {
  background: #8FBAF3;
  height: 20px;
  width: 100%;
  top: 0;
  left: 0;
  animation: lineTop 0.8s cubic-bezier(0.05, 0.79, 0.82, 0.31);
}
.square .line-right {
  background: #8FBAF3;
  height: 100%;
  width: 20px;
  top: 0;
  right: 0;
  animation: lineRight 0.8s cubic-bezier(0.05, 0.79, 0.82, 0.31);
}
.square .line-bottom {
  background: #8FBAF3;
  height: 20px;
  width: 100%;
  bottom: 0;
  left: 0;
  animation: lineBottom 0.8s cubic-bezier(0.05, 0.79, 0.82, 0.31);
}
.square .line-left {
  background: #8FBAF3;
  height: 100%;
  width: 20px;
  top: 0;
  left: 0;
  transform: translateY(60%);
  animation: lineLeft 0.8s cubic-bezier(0.05, 0.79, 0.82, 0.31);
}
.square .black-mask {
  width: 100%;
  height: 100%;
  transform: translateX(100%);
  animation: blackMask 1s 0.9s cubic-bezier(0.35, 1.1, 0.57, 0.76);
}
.square .text {
  color: #8FBAF3;
  margin-top:42rpx;
  background: none;
  font-size: 2.5em;
  line-height: 1;
  letter-spacing: 0px;
  text-transform: uppercase;
  opacity: 0;
  animation: moveIn 0s 1.4s cubic-bezier(0.35, 1.1, 0.57, 0.76) forwards;
}
